{
    "extends": ["airbnb-base", "prettier"],
    "rules": {
      "import/prefer-default-export": "off",
      "quotes": ["error", "double"],
      "import/extensions": "off",
      "import/no-unresolved": ["error", {
        "ignore": ["^electron/"]
      }],
      "no-undef": "off"
    },
    "env": {
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    }
}
